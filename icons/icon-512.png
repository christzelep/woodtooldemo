<button onclick="downloadIcon512()">Download Icon 512x512</button>

<script>
function downloadIcon512() {
  const link = document.createElement('a');
  link.href = "data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAgAAAA..."; // ολόκληρο base64 εδώ
  link.download = "icon-512x512.png";
  document.body.appendChild(link);
  link.click();
  document.body.removeChild(link);
}
</script>

